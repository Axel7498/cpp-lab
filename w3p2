//class 2
#include<iostream>
using namespace std;
class A{
	private: int x;
	public: void setx(int);
	int getx();
};

void A::setx(int n){
	x=n;//assigning the values of n to x
}
int A::getx(){
	return x;
}

int main(){
	A m1;
	A m2;
	m1.setx(5);
	m2.setx(6);
	int a=m1.getx();
	int b=m2.getx();
	cout<<a<<b;
	return 0;
}
